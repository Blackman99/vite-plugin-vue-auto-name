#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/dongsheng/文档/opay-h5-ui/node_modules/.pnpm/vite@2.8.6/node_modules/vite/bin/node_modules:/home/dongsheng/文档/opay-h5-ui/node_modules/.pnpm/vite@2.8.6/node_modules/vite/node_modules:/home/dongsheng/文档/opay-h5-ui/node_modules/.pnpm/vite@2.8.6/node_modules:/home/dongsheng/文档/opay-h5-ui/node_modules/.pnpm/node_modules:/home/dongsheng/文档/opay-h5-ui/node_modules:/home/dongsheng/文档/node_modules:/home/dongsheng/node_modules:/home/node_modules:/node_modules:/home/dongsheng/文档/opay-h5-ui/packages/vite-plugin-vue-auto-name/node_modules:/home/dongsheng/文档/opay-h5-ui/packages/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/dongsheng/文档/opay-h5-ui/node_modules/.pnpm/vite@2.8.6/node_modules/vite/bin/node_modules:/home/dongsheng/文档/opay-h5-ui/node_modules/.pnpm/vite@2.8.6/node_modules/vite/node_modules:/home/dongsheng/文档/opay-h5-ui/node_modules/.pnpm/vite@2.8.6/node_modules:/home/dongsheng/文档/opay-h5-ui/node_modules/.pnpm/node_modules:/home/dongsheng/文档/opay-h5-ui/node_modules:/home/dongsheng/文档/node_modules:/home/dongsheng/node_modules:/home/node_modules:/node_modules:/home/dongsheng/文档/opay-h5-ui/packages/vite-plugin-vue-auto-name/node_modules:/home/dongsheng/文档/opay-h5-ui/packages/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/vite@2.8.6/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/vite@2.8.6/node_modules/vite/bin/vite.js" "$@"
fi
